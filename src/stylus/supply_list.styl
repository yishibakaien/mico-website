.main .supply-list {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    padding: 12px;
    margin-top: 8px;
    background: #fff;
}
.main .supply-list .img-wrapper {
    -webkit-flex: 0 0 75px;
    flex: 0 0 75px;
    width: 75px;
    height: 75px;
    margin-right: 12px;
    overflow: hidden;
    // background-image: url('../images/patterns3.png');
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}
.main .supply-list .content {
    -webkit-flex: 1;
    flex: 1;
    position: relative;
}
.main .supply-list .content .title {
    margin-top: 3px;
    margin-bottom: 20px;
    padding-right: 16px;
    font-size: 15px;
    line-height: 18px;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
}
.main .supply-list .content .type {
    position: absolute;
    left: 0;
    bottom: 0;
    padding: 3px 6px;
    border-radius: 4px;
    font-size: 12px;
    color: #999;
    background: #f8f8f8;
}
.main .supply-list .content .time {
    position: absolute;
    right: 0;
    bottom: 0;
    font-size: 12px;
    color: #999;
}
.main .supply-list .icon-back {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    color: #666;
}

.more,
.no-more {
    margin 20px 0
    text-align center
    color #999
    line-height 40px
    height 40px
    background #fff
    display none
}
.more:active {
    color #4c93fd
}